<apex:page showHeader="false" sidebar="false" standardController="Account" tabstyle="Account" extensions="previous">  
 <apex:form >  
 <apex:sectionHeader title="Edit the record"/>  
 <apex:pageBlock tabStyle="Account">  
 <apex:pageBlockButtons location="both">  
 <apex:commandButton value="save"  onclick="windows.open('/apex/editdeletetable')" action="{!save}" />  
 </apex:pageBlockButtons>  
 <apex:pageBlockSection >  
 <apex:inputField value="{!Account.name}"/>  
 <apex:inputField value="{!Account.phone}"/>  
 <apex:inputField value="{!Account.Email__c}"/>  
 </apex:pageBlockSection>  
 </apex:pageBlock>  
 </apex:form>  
 </apex:page>