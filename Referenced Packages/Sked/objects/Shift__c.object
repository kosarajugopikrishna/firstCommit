    </validationRules>
    <validationRules>
        <fullName>DurationMustMatchStartToEnd</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISNULL( End1__c ) ) , NOT( ISNULL( Start__c ) ), Duration__c  &lt;&gt;  ROUND( (End1__c  -  Start__c) * 1440 ,0)  )</errorConditionFormula>
        <errorMessage>Duration must match Start to End</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartEndDurationAreRequired</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL( Start__c ) , ISNULL( End1__c ) , ISNULL( Duration__c ) )</errorConditionFormula>
        <errorMessage>Start, End and Duration are required</errorMessage>
    </validationRules>
