    <validationRules>
        <fullName>DurationMustBeGreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>Duration__c  &lt; 1</errorConditionFormula>
        <errorDisplayField>Duration__c</errorDisplayField>
        <errorMessage>A job&apos;s duration must be greater than 0 minutes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DurationMustMatchStartToFinish</fullName>
        <active>true</active>
        <description>Check to see that the duration is the amount of minutes between start and finish. Also enforces that start and finish are to the minute by virtue of the ROUND function</description>
        <errorConditionFormula>AND(
  NOT ISNULL(Finish__c), NOT ISNULL(Start__c), Duration__c &lt;&gt; ROUND(((Finish__c - Start__c) * 1440),0)  )</errorConditionFormula>
        <errorMessage>If Start and End are both set, then Duration must match</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DurationNoGreaterThan24Hrs</fullName>
        <active>true</active>
        <description>A job&apos;s duration can not be longer than 24 hrs</description>
        <errorConditionFormula>false</errorConditionFormula>
        <errorMessage>Duration cannot exceed 24 hours</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinishMustProvidedIfStartProvided</fullName>
        <active>true</active>
        <description>Either both Start and End are set, or both are blank.</description>
        <errorConditionFormula>AND (
  NOT ISNULL( Start__c ),ISNULL( Finish__c )
)</errorConditionFormula>
        <errorMessage>Both Start and Finish date must be provided or they both must be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoRRWithQuantityPresent</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Resource_Requirement_Count__c  &gt; 0 , NOT( ISNULL( Quantity__c ) ) )</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Job quantity must be null to have resource requirement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartEndDateCheck</fullName>
        <active>true</active>
        <errorConditionFormula>Start__c &gt; Finish__c</errorConditionFormula>
        <errorDisplayField>Start__c</errorDisplayField>
        <errorMessage>Start Date cannot be after the End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartMustProvidedIfFinishProvided</fullName>
        <active>true</active>
        <description>Either both Start and End are set, or both are blank.</description>
        <errorConditionFormula>AND (
  ISNULL( Start__c ),NOT ISNULL( Finish__c )

)</errorConditionFormula>
        <errorMessage>Both Start and Finish date must be provided or they both must be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateJobStatus</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISPICKVAL( Job_Status__c , &apos;Queued&apos;) || ISPICKVAL( Job_Status__c , &apos;Pending Allocation&apos;) || ISPICKVAL( Job_Status__c , &apos;Pending Dispatch&apos;) || ISPICKVAL( Job_Status__c , &apos;Dispatched&apos;) || ISPICKVAL( Job_Status__c , &apos;Ready&apos;) || ISPICKVAL( Job_Status__c , &apos;En Route&apos;) || ISPICKVAL( Job_Status__c , &apos;On Site&apos;) || ISPICKVAL( Job_Status__c , &apos;In Progress&apos;) || ISPICKVAL( Job_Status__c , &apos;Complete&apos;) || ISPICKVAL( Job_Status__c , &apos;Cancelled&apos;) )</errorConditionFormula>
        <errorDisplayField>Job_Status__c</errorDisplayField>
        <errorMessage>Invalid Job Status value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>notify_period_must_be_24_hours_or_more</fullName>
        <active>true</active>
        <description>Must be greater than 24 hours</description>
        <errorConditionFormula>NotifyPeriod__c &lt; 1</errorConditionFormula>
        <errorDisplayField>NotifyPeriod__c</errorDisplayField>
        <errorMessage>Notify Period must be 24 hours or more</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>notify_period_requires_start_date_time</fullName>
        <active>false</active>
        <description>when user enters NotifyPeriod, they must also enter Start</description>
        <errorConditionFormula>ISBLANK(Start__c)  &amp;&amp;  (ISBLANK( NotifyPeriod__c )  &lt;&gt; true)</errorConditionFormula>
        <errorDisplayField>Start__c</errorDisplayField>
        <errorMessage>Notify Period requires a valid Start</errorMessage>
    </validationRules>
</CustomObject>
